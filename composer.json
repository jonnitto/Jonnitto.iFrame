{
    "name": "jonnitto/iframe",
    "type": "neos-plugin",
    "description": "iFrames for Neos CMS",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "flow",
        "neos",
        "iframe",
        "contentelement"
    ],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "^5.3 || ^7.0 || ^8.0",
        "neos/fusion-afx": "^1.2 || ^7.0 || ^8.0",
        "carbon/eel": "^1.7 || ^2.0",
        "carbon/includeassets": "^3.5 || ^4.0 || ^5.0 || ^6.0",
        "carbon/notification": "^1.2 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\iFrame\\": "Classes"
        }
    },
    "archive": {
        "exclude": [
            ".editorconfig",
            ".github",
            ".gitattributes",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "extra": {
        "installer-name": "Jonnitto.iFrame",
        "neos": {
            "package-key": "Jonnitto.iFrame"
        }
    }
}
