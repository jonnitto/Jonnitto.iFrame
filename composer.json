{
    "name": "jonnitto/iframe",
    "type": "typo3-flow-plugin",
    "description": "iFrames for Neos CMS",
    "license": "MIT",
    "keywords": [
        "flow",
        "neos",
        "iframe",
        "contentelement"
    ],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/neos": "~2.0",
        "carbon/notification": "^0.1"
    },
    "autoload": {
        "psr-0": {
            "Jonnitto\\iFrame": "Classes"
        }
    },
    "archive": {
        "exclude": [
            ".editorconfig",
            ".github",
            ".gitattributes",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Jonnitto.iFrame",
        "neos": {
            "package-key": "Jonnitto.iFrame"
        }
    }
}
